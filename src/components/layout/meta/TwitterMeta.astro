---
import type { MetaProps } from 'types/meta';
export type Props = MetaProps;
const { title, url, description } = Astro.props;
---

<meta
  name='twitter:card'
  content={new URL('/social.png', url?.origin)}
/>
<meta
  property='twitter:domain'
  content='bdreece.dev'
/>
<meta
  property='twitter:url'
  content={url}
/>
<meta
  name='twitter:title'
  content={title}
/>
<meta
  name='twitter:description'
  content={description}
/>
<meta
  name='twitter:image'
  content={new URL('/social.png', url?.origin)}
/>
